{
    "name": "papposilene/geodata",
    "description": "A PHP package for countries, currencies & others little data",
    "keywords": [
        "geodata",
        "world",
        "earth",
        "continent",
        "continents",
        "subcontinent",
        "subcontinents",
        "country",
        "countries",
        "flag", "flags", 
        "currency", "currencies",
        "borders", "city", "cities",
        "geometry", "topology", "timezones"
    ],
    "homepage": "https://github.com/papposilene/geodata",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Philippe-Alexandre Pierre",
            "email": "dev@psln.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/container": "^8.0|^9.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Papposilene\\Geodata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Papposilene\\Geodata\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Papposilene\\Geodata\\GeodataServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
